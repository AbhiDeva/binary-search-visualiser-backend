{
    "version" :2,
    "builds": [
        {
            "src" : "mern-stack-binary-search/backend/server.js",
            "use" : "@vercel/node"
        }
    ], 
    "routes" : [
        {
            "src": "/api/(.*)",
            "dest" : "mern-stack-binary-search/backend/server.js"
        }, 
        {
            "src" : "/(.*)",
            "status": 404,
            "dest": "mern-stack-binary-search/backend/server.js"
        }
    ], 
    "env": {
        "NODE_ENV": "production"
    }
}